{
  "rewrites": [
    {
      "source": "/frendz",
      "destination": "/frendz/index.html"
    },
    {
      "source": "/frendz/:path*",
      "destination": "/frendz/:path*"
    },
    {
      "source": "/chatapp",
      "destination": "/chatapp/index.html"
    },
    {
      "source": "/chatapp/:path*",
      "destination": "/chatapp/:path*"
    }
  ],
  "headers": [
    {
      "source": "/frendz/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600, must-revalidate" }
      ]
    },
    {
      "source": "/chatapp/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/chatapp/sw.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    },
    {
      "source": "/chatapp/(.*).js",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/chatapp/styles/(.*).css",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/chatapp/data/(.*).json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Cache-Control", "value": "max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/chatapp/media/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
